<% if (products.length> 0) { %>
    <div>
        <table style="width: 50%;border-radius: 10%;background-color: azure;">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>

            <% products.forEach(product=> { %>
                <tr>
                    <td>
                        <%=product.title%>
                    </td>
                    <td>
                        <%=product.price%>
                    </td>
                    <td>
                        <img src=<%=product.thumbnail%> alt="<%=product.title%>" srcset="">
                    </td>
                </tr>
                <% }) %>
        </table>
    </div>
    <%} else {%>
        <span>No hay productos</span>
        <%} %>